{
  "name": "game-platform",
  "version": "0.4.0",
  "description": "Common utilitie for Canvas games",
  "keywords": [
    "game",
    "html5 games",
    "game platform",
    "game engine",
    "game canvas"
  ],
  "scripts": {
    "init": "npm install && jarb init",
    "start": "jarb devServer --env=develop",
    "build": "jarb build",
    "analyze": "jarb analyze --analyzeBundle=1",
    "test": "jarb test --coverage",
    "tdd": "jarb test",
    "releasePatch": "release-it",
    "releaseMinor": "release-it minor",
    "releaseMajor": "release-it major",
    "release": "jarb release"
  },
  "peerDependencies": {
    "react": "16.8.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "7.8.3",
    "jarb": "3.0.8",
    "jest-canvas-mock": "^2.1.1",
    "regenerator-runtime": "^0.13.3"
  },
  "files": [
    "dist",
    "types"
  ],
  "main": "dist",
  "dependencies": {
    "@types/jest": "^25.1.2",
    "canvas": "2.6.0",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "ts-jest": "^25.2.0",
    "typescript": "^3.7.5"
  },
  "types" : "types/index.d.ts"
}
